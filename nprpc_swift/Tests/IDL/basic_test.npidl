// Simple test IDL for Swift bindings
module swift.test;

exception TestException {
    message: string;
    code: u32;
}


namespace TestNamespace {
  interface TestInterface {
    void testMethod1(arg1: f32);
  }
}

enum Color {
  Red,
  Green,
  Blue
}

message Point {
  x: i32;
  y: i32;
  z?: i32;
  symbol: string;
}

message Rectangle {
  topLeft: Point;
  bottomRight: Point;
  color: Color;
}

interface ShapeService {
  void getRectangle(id: u32, rect: out Rectangle);
  void setRectangle(id: u32, rect: Rectangle);
  void getRectangles(rects: out vector<Rectangle>);
  void getNumbers(nums: out vector<i32>);
  void throwingMethod(code: u32) raises (TestException);
}

namespace Namespace1 {
  message Message1 {
    field1: string;
    field2: i32;
  }

  interface Service1 {
    void method1(msg: Message1);
    void method2(arg1: f32);
  }
}
