
module nprpc.node;

alias bytes = vector<u8>;

message Header {
  key: string;
  value: string;
}

[force_helpers]
message SSRRequest {
  request_id: u64;
  method: string;
  url: string;
  headers: vector<Header>;
  body: bytes;
  client_address: string;
}

[force_helpers]
message SSRResponse {
  request_id: u64;
  status_code: i32;
  headers: vector<Header>;
  body: bytes;
}
